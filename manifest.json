{
  "name": "Commute Routes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Show commute options from address in clipboard",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "options_page": "options.html",

  "background": {
	"page": "background.html"
  },

  "permissions": [
    "clipboardRead",  // To get source address
    "tabs"            // To close the "Options" tab
  ]
}







